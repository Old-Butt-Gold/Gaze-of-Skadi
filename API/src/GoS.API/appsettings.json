{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenDotaRequester": {
    "BaseAddress": "https://api.opendota.com/api/",
    "TimeoutSeconds": 30,
    "ApiKey": "",
    "RetryCount": 3,
    "RetryMedianDelaySeconds": 1.0,
    "CircuitBreakerFailureThreshold": 5,
    "CircuitBreakerBreakDurationSeconds": 30
  },
  "SteamRequester": {
    "BaseAddress": "https://api.steampowered.com/",
    "TimeoutSeconds": 30,
    "ApiKey": "",
    "RetryCount": 3,
    "RetryMedianDelaySeconds": 1.0,
    "CircuitBreakerFailureThreshold": 5,
    "CircuitBreakerBreakDurationSeconds": 30
  },
  "StratzRequester": {
    "BaseAddress": "https://api.stratz.com/graphql",
    "TimeoutSeconds": 30,
    "ApiKey": "",
    "RetryCount": 3,
    "RetryMedianDelaySeconds": 1.0,
    "CircuitBreakerFailureThreshold": 5,
    "CircuitBreakerBreakDurationSeconds": 30
  },
  "Redis": {
    "Enabled": false,
    "Configuration": "localhost:6379",
    "InstanceName": "GoS:"
  }
}
